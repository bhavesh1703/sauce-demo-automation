<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="regressionSuite" parallel="tests" thread-count="2">

    <listeners>
        <listener class-name="listeners.TestListener"></listener>
    </listeners>
    
    <test name="regressionTests">
        <groups>
            <run>
                <include name="regression"/>
            </run>
        </groups>
        <classes>
            <class name="tests.LoginTest"></class>
            <class name="tests.ProductsPageTest"></class>
        </classes>
    </test>
</suite>